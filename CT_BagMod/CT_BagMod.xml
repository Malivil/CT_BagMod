                        <Texture name="$parentEditBoxRight" file="Interface\ClassTrainerFrame\UI-ClassTrainer-FilterBorder">
                            <Size>
                                <AbsDimension x="12" y="29"/>
                            </Size>
                            <Anchors>
                                <Anchor point="LEFT" relativeTo="$parentEditBoxMiddle" relativePoint="RIGHT"/>
